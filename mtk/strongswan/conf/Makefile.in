install-optionstemplateDATA: $(optionstemplate_DATA)
	@$(NORMAL_INSTALL)
	@list='$(optionstemplate_DATA)'; test -n "$(optionstemplatedir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(optionstemplatedir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(optionstemplatedir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(optionstemplatedir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(optionstemplatedir)" || exit $$?; \
	done

