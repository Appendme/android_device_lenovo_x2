#
# system.prop for x2eu
#
service.adb.root=1

persist.mtk.aee.aed=on


# Display
ro.sf.lcd_density=480
ro.opengles.version=196608
# seems dirty region is not supported by gpu according to logcat
debug.hwui.render_dirty_regions=false
ro.hwui.texture_cache_size=48
ro.hwui.layer_cache_size=32
ro.hwui.r_buffer_cache_size=8
ro.hwui.path_cache_size=32
ro.hwui.gradient_cache_size=3
ro.hwui.drop_shadow_cache_size=6
ro.hwui.fbo_cache_size=25
ro.hwui.texture_cache_flushrate=0.4
ro.hwui.text_small_cache_width=1024
ro.hwui.text_small_cache_height=1024
ro.hwui.text_large_cache_width=2048
ro.hwui.text_large_cache_height=1024
ro.hwui.disable_asset_atlas=true

# show software keys
qemu.hw.mainkeys=1

# WiFi
wifi.interface=wlan0
wifi.tethering.interface=ap0
wifi.direct.interface=p2p0
ro.mediatek.wlan.wsc=1
ro.mediatek.wlan.p2p=1
mediatek.wlan.ctia=0

persist.mtk.wcn.combo.chipid=-1
service.wcn.driver.ready=no

mediatek.wlan.chip=MT6630
mediatek.wlan.module.postfix=_mt6630

ro.mtk_wlan_support=1
ro.mtk_tetheringipv6_support=1


# Audio
ro.camera.sound.forced=0
ro.audio.silent=0


# Telephony
persist.radio.fd.counter=15
persist.radio.fd.off.counter=5
persist.radio.fd.r8.counter=15
persist.radio.fd.off.r8.counter=5
fmradio.driver.enable=0

mux.debuglog.enable=0
rild.libpath=/system/lib/mtk-ril.so
rild.libargs=-d /dev/ttyC0
ril.specific.sm_cause=0
ril.external.md=0
ril.current.share_modem=1
ril.first.md=1
ril.flightmode.poweroffMD=0
ril.telephony.mode=0
ril.radiooff.poweroffMD=0

ro.telephony.sim.count=1
ro.telephony.ril_class=MediaTekRIL
ro.telephony.ril.config=fakeiccid

ro.mediatek.gemini_support=false
ro.gemini.smart_sim_switch=false
ro.sim_refresh_reset_by_modem=1
ro.sim_me_lock_mode=0
ro.mtk_enable_md1=1
ro.mtk_fd_support=1
ro.mtk_sim_hot_swap=1
ro.mtk_world_phone=1
ro.mtk_world_phone_policy=0
ro.mtk_lte_support=1
ro.operator.optr=CUST

persist.radio.default.sim=0
persist.radio.multisim.config=ss
persist.sys.memopt.switch=1
persist.md.perm.checked=to_upgrade
persist.gemini.sim_num=1

bgw.current3gband=0


# USB
ro.sys.usb.storage.type=mtp

# USB BICR function
ro.sys.usb.bicr=yes

# USB Charge only function
ro.sys.usb.charging.only=yes


# IPO
# Power off opt in IPO
sys.ipo.pwrdncap=2


# begin unrefined build properties
#ro.zygote.preload.enable=0

ro.kernel.zio=38,108,105,16
#ro.kernel.qemu=1
#ro.kernel.qemu.gles=0
#ro.boot.selinux=disable
#
ro.btstack=blueangel
ro.sf.hwrotation=0
drm.service.enabled=true
fmradio.driver.enable=0


